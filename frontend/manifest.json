{
  "name": "Deceit Defender",
  "manifest_version": 3,
  "version": "1.0",
  "description": "DPBH",
  "permissions": ["tabs", "activeTab", "contextMenus", "notifications"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "32": "logo.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "logo.png"
  }
}
